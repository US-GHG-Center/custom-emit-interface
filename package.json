{
  "name": "interface-template",
  "version": "1.0.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "yarn reset &&  dotenv -- node ./run-parcel.js serve",
    "serve": "parcel public/index.html",
    "reset": "yarn clean && yarn copy-static",
    "copy-static": "cp -r static/* public/",
    "clean": "rm -rf node_modules build yarn.lock .parcel-cache && yarn cache clean && yarn install",
    "build": "yarn reset &&  dotenv -- node ./run-parcel.js build --dist-dir build",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "fix-conflicts": "prettier --write . && eslint . --fix"
  },
  "dependencies": {
    "@date-io/moment": "^3.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/x-date-pickers": "^7.22.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@turf/turf": "^7.1.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "chart.js": "^4.4.3",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-plugin-annotation": "^3.0.1",
    "chartjs-plugin-zoom": "^2.0.1",
    "d3": "^7.9.0",
    "luxon": "^3.5.0",
    "mapbox-gl": "^3.7.0",
    "mapboxgl-timeline": "^1.0.2",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-loader-spinner": "^6.1.6",
    "react-resizable-panels": "^2.1.1",
    "react-router-dom": "^6.27.0",
    "styled-components": "^6.1.13",
    "typescript": "^4.4.2",
    "uuid": "^11.0.3",
    "web-vitals": "^2.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.15.0",
    "@parcel/transformer-webmanifest": "2.15.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "parcel": "^2.14.4",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "prettier": "^3.4.2"
  }
}
